{
  "name": "Mass Unfriender for Facebook™",
  "version": "2",
  "manifest_version": 2,
  "description": "Remove all Facebook™ friends in few clicks.",
  "icons": {
    "19": "icons/icon12888.png",
    "128": "icons/icon12888.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "js/jquery.min.js",
      "src/bg/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icons/icon12888.png",
    "default_title": "browser action demo"
  },
  "permissions": [
    "https://*.facebook.com/*"
  ]
}