{
  "name": "Mass Friends Remover for Facebook | mobile",
  "version": "16",
  "manifest_version": 2,
  "description": "Unfriend all Facebook friends in one click",
  "icons": {
    "19": "icons/icon12888.png",
    "128": "icons/icon12888.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "js/browser-polyfill.min.js",
      "js/jquery.min.js",
      "src/bg/background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/icon12888.png",
    "default_popup": "src/browser_action/browser_action.html",
    "default_title": "Mass Unfriender"
  },
  "permissions": [
    "activeTab",
    "https://*.facebook.com/*"
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}